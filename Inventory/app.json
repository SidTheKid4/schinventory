{
  "expo": {
    "name": "SCH Inventory",
    "slug": "Inventory",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/inventoryicon.png",
    "splash": {
      "image": "./assets/splashinventory.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.SpaceCenterHouston.Inventory"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.SpaceCenterHouston.Inventory"
    },
    "description": "Inventory organizer and item checkout for SCH"
  }
}